#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir//bin/env" ]; then
  exec "$basedir//bin/env" node "$basedir/../jsx-node/bin/jsxc.js" "$@"
else 
  exec /bin/env node "$basedir/../jsx-node/bin/jsxc.js" "$@"
fi
